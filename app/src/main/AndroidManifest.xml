<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nus.alchemy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/rounded_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rounded_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChatActivity"></activity>
        <activity android:name=".MatchActivity" />
        <activity android:name=".MyEventsActivity" />
        <activity android:name=".MyChatsActivity" />
        <activity android:name=".TodaysEventsActivity" />
        <activity android:name=".ForgotActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>